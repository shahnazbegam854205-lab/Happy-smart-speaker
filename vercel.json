{
    "version": 2,
    "builds": [
        {
            "src": "*.html",
            "use": "@vercel/static"
        },
        {
            "src": "*.js",
            "use": "@vercel/static"
        },
        {
            "src": "*.json",
            "use": "@vercel/static"
        }
    ],
    "routes": [
        {
            "src": "/sw.js",
            "headers": {
                "Cache-Control": "no-cache, no-store, must-revalidate",
                "Content-Type": "application/javascript"
            }
        },
        {
            "src": "/(.*)",
            "dest": "/index.html"
        }
    ]
}
